{
  "name": "webpack-threejs-boilerplate",
  "repository": "fazeaction/webpack-threejs-boilerplate",
  "scripts": {
    "dev": "webpack serve --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "lint": "eslint --ext .js src",
    "build": "node build/build.js"
  },
  "license": "WTFPL",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-glslify": "^2.0.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^5.0.0",
    "eslint": "^7.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-webpack-plugin": "^2.1.0",
    "exports-loader": "^1.1.1",
    "file-loader": "^6.1.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "glslify": "^7.1.1",
    "glslify-loader": "^2.0.0",
    "html-webpack-plugin": "^4.5.0",
    "ify-loader": "^1.1.0",
    "json-loader": "^0.5.4",
    "mini-css-extract-plugin": "^1.1.1",
    "node-notifier": "^8.0.1",
    "ora": "^5.1.0",
    "portfinder": "^1.0.28",
    "postcss-import": "^13.0.0",
    "postcss-loader": "^4.0.4",
    "postcss-url": "^8.0.0",
    "raw-loader": "^4.0.2",
    "shelljs": "^0.8.4",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.1.3",
    "webpack-bundle-analyzer": ">=3.9.0",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.2.0",
    "worker-loader": "^3.0.5"
  },
  "dependencies": {
    "@superguigui/wagner": "git+https://github.com/fazeaction/Wagner.git#master",
    "dat-gui": "^0.5.0",
    "glsl-checker": "^1.0.1",
    "glsl-noise": "0.0.0",
    "glslify-fancy-imports": "^1.0.1",
    "glslify-hex": "^2.0.1",
    "three": "^0.121.0",
    "three.fbo-helper": "^1.1.1",
    "webvr-polyfill": "^0.10.12",
    "webvr-ui": "^0.10.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "plugins": [
            "glslify"
          ]
        }
      ],
      "glslify"
    ]
  },
  "glslify": {
    "transform": [
      "glslify-fancy-imports",
      "glslify-hex"
    ]
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  }
}
